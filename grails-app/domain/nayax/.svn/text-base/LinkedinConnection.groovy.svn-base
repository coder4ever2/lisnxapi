package nayax

class LinkedinConnection {
	
	
	String userLinkedinId
	String url
	String firstName
	String lastName
	String pictureUrl
	String connectionLinkedinId
	String headline
	String industry
	String locationName
	Date dateCreated
	Date lastUpdated
	
	static constraints = {
		url(nullable:true)
		firstName(nullable:true)
		lastName(nullable:true)
		pictureUrl(nullable:true)
		connectionLinkedinId(nullable:false)
		headline(nullable:true)
		industry(nullable:true)
		locationName(nullable:true)
		userLinkedinId(nullable:false)
		dateCreated(nullable:true)
		lastUpdated(nullable:true)
	}
	
}
